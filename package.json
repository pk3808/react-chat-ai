{
  "name": "react-chat-ai",
  "version": "1.1.9",
  "description": "A powerful React component to easily add an AI chat assistant to your website. Supports Google Gemini and OpenAI.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:js && npm run cleanup:temp",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "webpack --mode production",
    "cleanup:temp": "if exist dist\\temp rmdir /s /q dist\\temp",
    "dev": "webpack serve --mode development",
    "test": "echo \"No test specified\"",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "keywords": [
    "react",
    "chatbot",
    "ai",
    "ai-chatbot",
    "react-component",
    "nextjs",
    "gemini",
    "openai",
    "customizable"
  ],
  "author": {
    "name": "Piyush Kumar",
    "url": "https://github.com/pk3808"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/pk3808/react-chat-ai.git"
  },
  "bugs": {
    "url": "https://github.com/pk3808/react-chat-ai/issues"
  },
  "homepage": "https://github.com/pk3808/react-chat-ai#readme"
}
